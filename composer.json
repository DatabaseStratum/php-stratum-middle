{
  "name": "setbased/php-stratum-middle",
  "description": "PhpStratum Middle: The glue between a PhpStratum backend and your application",
  "keywords": [
    "PhpStratum"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "setbased/exception": "^1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0.0 || ^8.0.0",
    "phing/phing": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Stratum\\Middle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Stratum\\Middle\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
